{
  "name": "fns-api",
  "version": "1.0.7",
  "description": "Methods of working with FNS API",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run del",
    "build": "tsc -p tsconfig.build.json",
    "prewatch": "npm run del",
    "watch": "tsc -p tsconfig.build.json --watch",
    "del": "rimraf dist",
    "format": "prettier --write \"{lib,tests,examples}/**/*.ts\"",
    "lint": "eslint \"{lib,tests,examples}/**/*.ts\" --fix",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "axios": ">= 0.27.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-angular": "^18.4.3",
    "@types/jest": "^29.5.9",
    "@types/node": "^20.9.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/fns-api.git"
  },
  "keywords": [
    "fns",
    "api",
    "axios"
  ],
  "author": "Nikolay Naumenkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikolaynau/fns-api/issues"
  },
  "homepage": "https://github.com/nikolaynau/fns-api#readme"
}
