{
  "name": "fns-api",
  "version": "1.0.2",
  "description": "Methods of working with FNS API",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run del",
    "build": "tsc -p tsconfig.build.json",
    "prewatch": "npm run del",
    "watch": "tsc -p tsconfig.build.json --watch",
    "del": "rimraf dist",
    "format": "prettier --write \"{lib,tests}/**/*.ts\"",
    "lint": "eslint \"{lib,tests}/**/*.ts\" --fix",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "postversion": "npm run push",
    "push": "git push origin master --tags"
  },
  "peerDependencies": {
    "axios": "^0.26.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/fns-api.git"
  },
  "keywords": [
    "fns",
    "api",
    "axios"
  ],
  "author": "Nikolay Naumenkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikolaynau/fns-api/issues"
  },
  "homepage": "https://github.com/nikolaynau/fns-api#readme"
}
